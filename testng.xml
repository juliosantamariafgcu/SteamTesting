<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Steam Testing" parallel="tests" thread-count="2">
  <test name="Sign-In Page">
    <classes>
      <class name="testing.steam.SignIn" />
    </classes>
  </test>
  <test name="Support Page">
    <classes>
      <class name="testing.steam.AuthenticatedSupport" />
      <class name="testing.steam.UnauthenticatedSupport" />
    </classes>
  </test>
  <test name="Wishlist">
    <classes>
      <class name="testing.steam.Wishlist" />
    </classes>
  </test>
  <test name="Search Filters">
    <classes>
      <class name="testing.steam.SearchFilters" />
    </classes>
  </test>
  <test name="Adding Game">
    <classes>
      <class name="testing.steam.AddingGameTest" />
    </classes>
  </test>
  <test name="Deleting Game">
    <classes>
      <class name="testing.steam.DeletingGameTest" />
    </classes>
  </test>
  <test name="Store Tab">
    <classes>
      <class name="testing.steam.StoreTabSteamTest" />
    </classes>
  </test>
</suite>
